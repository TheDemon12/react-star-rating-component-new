{
  "name": "react-star-rating-component",
  "version": "1.4.1",
  "description": "React component for star (or any other icon based) ratings",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-babel && npm run build-dist-dev && npm run build-dist-prod",
    "build-dist-dev": "NODE_ENV=development webpack --config webpack.config --colors",
    "build-dist-prod": "NODE_ENV=production webpack --config webpack.config --progress --colors -p",
    "build-babel": "babel ./src/StarRatingComponent.jsx --out-file ./index.js",
    "build-example": "browserify ./example/index.js -t [ babelify --presets [ @babel/env @babel/react ] --plugins [ @babel/transform-object-assign ] ] > ./example/bundle.js && browserify ./example/react-redux.js -t [ babelify --presets [ @babel/env @babel/react ] --plugins [ @babel/transform-object-assign ] ] > ./example/react-redux-bundle.js",
    "lint": "eslint ./src/* --fix",
    "test": "echo \"Comming soon ;)\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/networkandsoftware/react-star-rating-component.git"
  },
  "keywords": [
    "react",
    "component",
    "star",
    "rating",
    "rate",
    "input"
  ],
  "author": "Michiel van Wessem <windsunrain@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/networkandsoftware/react-star-rating-component/issues"
  },
  "homepage": "https://github.com/networkandsoftware/react-star-rating-component#readme",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babelify": "^10.0.0",
    "browserify": "^16.1.0",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "react-dom": "^16.2.0",
    "uglify-js": "^3.3.12",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.3.9",
    "webpack-umd-external": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4"
  }
}
